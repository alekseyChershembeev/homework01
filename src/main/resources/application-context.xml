<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="message" class="com.example.demo.controller.MessageImpl">

    </bean>
    <bean id="parser" class="com.example.demo.service.csvParser.ParserImpl">
    </bean>
    <bean id="tester" class="com.example.demo.service.tester.TestImpl">
        <constructor-arg name="message" ref="message"/>
        <constructor-arg name="parser" ref="parser"/>
    </bean>
</beans>
